.awards-block
  padding: 30px 0
  h2
    text-align center
    margin-bottom 54px
  .logo-wrapper  
    justify-content space-between
    column-gap: 20px
    margin-bottom 60px
  .logo-item 
    max-width: 300px
  .wp-block-image
    margin-bottom 24px 
    img
      width 200px !important
      height 200px
      object-fit: contain
  ul
    margin-left 30px

+media-mobile()    
  .awards-block
    .logo-wrapper  
      display block
    .logo-item  
      width 100%
      margin 0 auto 16px