.svg
  &._fill-primary
    fill $primary
  &._fill-secondary
    fill $secondary
  &._fill-darker
    fill $darker
  &._stroke-primary
    stroke $primary
  &._stroke-secondary
    stroke $secondary

.svg-templates
  position absolute
  overflow hidden
  clip rect(0 0 0 0)
  margin -1px
  padding 0
  width 1px
  height 1px
  border none
